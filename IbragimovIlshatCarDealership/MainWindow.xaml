<Window x:Class="IbragimovIlshatCarDealership.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IbragimovIlshatCarDealership"
        mc:Ignorable="d"
        Title="IbragimovIlshat_CarDealership" MinHeight="750" MinWidth="1400" FontFamily="Times New Roman" Icon="/image/car.png" Background="#8db3c9">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="30"></RowDefinition>
        </Grid.RowDefinitions>

        <Image Source="/image/car_noback.png" HorizontalAlignment="Left" Margin="5 0 0 0"></Image>
        <TextBlock Text="ЧестАвто" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="30"></TextBlock>
        <Button Content="Назад" Name="btnBack" HorizontalAlignment="Right" VerticalAlignment="Center" Click="Button_Click"></Button>

        <Frame Name="MainFrame" Grid.Row="1" NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered" Background="White"></Frame>
    </Grid>
</Window>
